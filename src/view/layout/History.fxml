<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.HistoryCtrl">
   <children>
      <VBox alignment="CENTER" prefHeight="349.0" prefWidth="238.0" spacing="20.0" AnchorPane.leftAnchor="300.0" AnchorPane.topAnchor="300.0">
         <children>
            <Label fx:id="history" alignment="CENTER" opacity="0.49" prefHeight="261.0" prefWidth="238.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="txt" alignment="CENTER" promptText="enter game name" />
            <Button fx:id="start" alignment="CENTER" mnemonicParsing="false" onAction="#onStartClicked" text="start" />
         </children>
      </VBox>
   </children>
</AnchorPane>
